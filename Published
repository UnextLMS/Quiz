Payload to Target All Members under a course and program plan

[
    {
        "keys": {
           "UniqueId": "{{$guid}}" //Random Generated Id UUID/GUID
        },
        "values": 
        {
         "Target": "All",
         "QuizTitle": "ML Discussion",
         "QuizLink": "www.mldf.com",
         "CourseOfferingId": "SF 18 digit Id",
         "QuizId":"HI78",
         "CreatedDate":"04-10-2022" //Quiz Created date in LMS
         "StartDate": "04-12-2022",
         "EndDate": "04-14-2022",
         "Action": "Published",
         "Choice":"Mandatory",
         "Type":"Gradable"
          
         }

    }
]


